{
  "name": "regbot",
  "version": "1.0.0",
  "type": "module",
  "imports": {
    "#root/*": "./build/src/*"
  },
  "scripts": {
    "build": "tsc && NODE_ENV=development npx mikro-orm cache:generate --combined && cp temp/metadata.json build/metadata.json",
    "clean": "rm -rf build node_modules temp",
    "dev": "tsc-watch --onSuccess \"tsx ./src/main.ts\"",
    "format": "eslint . --fix",
    "lint": "eslint .",
    "make-migrations": "tsc && npx mikro-orm migration:create",
    "migrate": "tsc && npx mikro-orm migration:up",
    "prepare": "husky || true",
    "start": "tsc && tsx ./src/main.ts",
    "start:prod": "node ./build/src/main.js"
  },
  "author": "Tamika Nomara <taminomara@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@grammyjs/auto-chat-action": "0.1.1",
    "@grammyjs/conversations": "^1.2.0",
    "@grammyjs/hydrate": "1.4.1",
    "@grammyjs/i18n": "1.0.2",
    "@grammyjs/menu": "^1.2.1",
    "@grammyjs/parse-mode": "1.10.0",
    "@grammyjs/types": "3.7.0",
    "@mikro-orm/better-sqlite": "^6.2.7",
    "@mikro-orm/core": "^6.2.7",
    "@mikro-orm/migrations": "^6.2.7",
    "callback-data": "1.1.1",
    "express": "^4.19.2",
    "grammy": "^1.23.0",
    "grammy-guard": "0.5.0",
    "iso-639-1": "3.1.2",
    "moment-timezone": "^0.5.45",
    "pino": "9.1.0",
    "pino-pretty": "11.0.0",
    "prom-client": "^15.1.2",
    "sanitize-html": "^2.13.0",
    "znv": "0.4.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^6.2.7",
    "@mikro-orm/reflection": "^6.2.7",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/express": "^4.17.21",
    "@types/node": "20.12.12",
    "@types/sanitize-html": "^2.11.0",
    "@typescript-eslint/eslint-plugin": "7.9.0",
    "@typescript-eslint/parser": "7.9.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-unicorn": "53.0.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "3.2.4",
    "tsc-watch": "6.2.0",
    "tsx": "4.10.4",
    "typescript": "5.4.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "lint-staged": {
    "*.ts": "eslint"
  },
  "mikro-orm": {
    "configPaths": [
      "./build/src/backend/micro-orm-config.js"
    ]
  }
}
